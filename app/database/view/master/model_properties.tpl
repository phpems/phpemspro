{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid">		<div class="pep">			<div class="col-xs-2 leftmenu">				{x2;include:menu}			</div>			<div class="col-xs-10" id="datacontent">				<ol class="breadcrumb">					<li><a href="index.php?{x2;$_route['app']}-master">{x2;$apps[$_route['app']]['appname']}</a></li>					<li><a href="index.php?{x2;$_route['app']}-master-model">模型管理</a></li>					<li class="active">属性</li>				</ol>				<div class="panel panel-default">					<div class="panel-heading">属性（{x2;$model['modelname']}）<a class="pull-right" href="index.php?database-master-model-addproperty&modelcode={x2;$model['modelcode']}">增加</a></div>					<div class="panel-body">						<form action="index.php?database-master-model-orders" method="post" class="form-horizontal">							<table class="table table-striped table-bordered">								<tr>									<th width="40">排序</th>									<th width="80">属性ID</th>									<th width="160">属性名</th>									<th width="120">绑定字段</th>									<th width="120">HTML类型</th>									<th width="80">状态</th>									<th>备注</th>									<th width="100">操作</th>								</tr>								{x2;if:$properties}								{x2;tree:$properties,property,pid}								<tr>									<td class="form-inline">										<input type="text" name="ids[{x2;v:property['ppyid']}]" value="{x2;v:property['ppyorder']}" class="orderinput"/>									</td>									<td>{x2;v:property['ppyid']}</td>									<td>{x2;v:property['ppyname']}</td>									<td>{x2;v:property['ppyfield']}</td>									<td>{x2;v:property['ppyhtmltype']}</td>									<td>{x2;if:v:property['ppyislock']}禁用{x2;else}正常{x2;endif}</td>									<td>{x2;v:property['ppyintro']}</td>									<td>										<ul class="list-unstyled list-inline">											<li><a href="index.php?{x2;$_route['app']}-master-model-modifyproperty&ppyid={x2;v:property['ppyid']}">修改</a></li>											{x2;if:!v:property['ppysystem']}											<li><a class="confirm" href="index.php?{x2;$_route['app']}-master-model-delproperty&ppyid={x2;v:property['ppyid']}">删除</a></li>											{x2;endif}										</ul>									</td>								</tr>								{x2;endtree}								{x2;else}								<tr>									<td colspan="8">该模型下没有属性</td>								</tr>								{x2;endif}							</table>							<div class="form-group">								<div class="col-sm-4">									<button class="btn btn-primary" type="submit">排序</button>									<input type="hidden" name="orderproperty" value="1"/>								</div>							</div>						</form>					</div>				</div>			</div>		</div>	</div></div>{x2;include:footer}</body></html>