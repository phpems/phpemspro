{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid panels">		<div class="pep panels">			<div class="col-xs-2">				{x2;include:menu}			</div>			<div class="col-xs-10 nopadding">				<div class="pagebox border padding bg">					<div class="col-xs-9">						<div style="clear: both;overflow: hidden">                            {x2;eval: v:rate = intval(100*($allnumber['right']+$allnumber['wrong'])/$allnumber['all'])}							<div class="col-xs-2">做题进度</div>							<div class="col-xs-7">								<div class="progress">									<div class="progress-bar" role="progressbar" aria-valuenow="{x2;v:rate}" aria-valuemin="0" aria-valuemax="100" style="width: {x2;v:rate}%;">									</div>								</div>							</div>							<div class="col-xs-3" style="font-size: 14px">								<span class="text-success"> 已做：{x2;eval: echo $allnumber['right']+$allnumber['wrong']}</span> / 总数：{x2;$allnumber['all']}							</div>						</div>						<div style="clear: both;overflow: hidden">							<div class="col-xs-2">正确率</div>                            {x2;eval: v:rate = intval(100*$allnumber['right']/($allnumber['right']+$allnumber['wrong']))}							<div class="col-xs-7">								<div class="progress" style="margin-bottom: 0px;">									<div class="progress-bar" role="progressbar" aria-valuenow="{x2;v:rate}" aria-valuemin="0" aria-valuemax="100" style="width: {x2;v:rate}%;">									</div>								</div>							</div>							<div class="col-xs-3" style="font-size: 14px">								正确率：{x2;v:rate}%							</div>						</div>					</div>					<div class="col-xs-3 text-right">                        {x2;if:$point}						<a class="btn btn-primary" style="margin-top: 10px;" href="index.php?exam-app-point-paper&pointid={x2;$point['pointid']}">继续上次练习</a>						{x2;else}						<a class="btn btn-primary" style="margin-top: 10px;" href="index.php?exam-app-point-paper&pointid={x2;eval: echo current(current($basic['basicpoints']))}">开始练习</a>						{x2;endif}					</div>				</div>				<div class="panel panel-default pagebox border margin">					<div class="panel-heading bold">						章节练习					</div>					<div class="panel-body" style="padding: 0px">						<div class="panel-group" id="panel-13465">                            {x2;tree:$basic['basicsections'],section,sid}							<div class="panel">								<div class="panel-heading blod">									<a class="panel-toggle text-center" data-parent="#panel-13465" data-toggle="collapse" href="#panel-element-{x2;v:sid}">                                        {x2;$sections[v:section]['sectionname']}									</a>								</div>								<div id="panel-element-{x2;v:sid}" class="panel-collapse{x2;if:v:sid <= 1} in{x2;endif} collapse" role="tabpanel">									<ul class="list-group">                                        {x2;tree:$basic['basicpoints'][v:section],point,pid}										<li class="list-group-item">											<a href="index.php?exam-app-point&pointid={x2;v:point}">                                                {x2;$points[v:section][v:point]['pointname']}											</a>											<a class="btn btn-default pull-right" href="index.php?exam-app-point&pointid={x2;v:point}">开始做题</a>											<span class="btn pull-right btn-progress" style="width: 20rem;">												{x2;eval: v:rate = intval(100 * $donenumber[v:point]/$numbers[v:point])}												<div class="progress pull-left">													<div class="progress-bar" role="progressbar" aria-valuenow="{x2;v:rate}" aria-valuemin="0" aria-valuemax="100" style="width: {x2;v:rate}%;"></div>												</div>												<span>{x2;$donenumber[v:point]} / {x2;$numbers[v:point]}</span>											</span>										</li>										{x2;endtree}									</ul>								</div>							</div>                            {x2;endif}						</div>					</div>				</div>			</div>		</div>	</div></div>{x2;include:footer}</body></html>