{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid panels">		<div class="pep panels">			<div class="col-xs-2">				{x2;include:menu}			</div>			<div class="col-xs-10 nopadding">				<div class="pagebox border padding bg">					<div class="col-xs-3">						<p class="text-center">收藏</p>						<p class="text-center text-warning">{x2;$favor}题</p>					</div>					<div class="col-xs-3">						<p class="text-center">笔记</p>						<p class="text-center text-success">{x2;$note}题</p>					</div>					<div class="col-xs-3">						<p class="text-center">已做</p>						<p class="text-center text-danger">{x2;eval: echo $allnumber['wrong']+$allnumber['right']}题</p>					</div>					<div class="col-xs-3">						<p class="text-center">总题数</p>						<p class="text-center text-info">{x2;eval: echo $allnumber['all']}题</p>					</div>				</div>				<div class="panel panel-default pagebox border margin">					<div class="panel-heading bold">						考试趋势					</div>					<div class="panel-body">						<div style="width:100%;height: 360px;" class="exam-charts"></div>					</div>				</div>			</div>		</div>	</div></div><script>    $(function(){        var option = {            xAxis: {                type: 'category',                data: [{x2;tree:$histories['data'],history,hid}{x2;eval: v:history['ehstarttime'] = date('m-d',strtotime(v:history['ehstarttime']));}{x2;if:v:key},'{x2;v:history['ehstarttime']}'{x2;else}'{x2;v:history['ehstarttime']}'{x2;endif}{x2;endtree}]            },            yAxis: {                type: 'value'            },            series: [{                data: [{x2;tree:$histories['data'],history,hid}{x2;if:v:key},{x2;eval:echo intval(v:history['ehscore'])}{x2;else}{x2;eval:echo intval(v:history['ehscore'])}{x2;endif}{x2;endtree}],                type: 'line'            }]        };        echarts.init($('.exam-charts:first')[0]).setOption(option);    });</script>{x2;include:footer}</body></html>