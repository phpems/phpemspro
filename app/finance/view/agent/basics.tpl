{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid">		<div class="pep">			<div class="col-xs-2 leftmenu">				{x2;include:menu}			</div>			<div class="col-xs-10" id="datacontent">				<ol class="breadcrumb">					<li><a href="index.php?{x2;$_route['app']}-agent">代理平台</a></li>					<li class="active">开通课程</li>				</ol>				<div class="settingbar">					{x2;tree:$trainings,training,tid}					<a href="index.php?finance-agent-basics&trid={x2;v:training['trid']}" class="btn{x2;if:$trid == v:training['trid']} btn-primary{x2;else} btn-default{x2;endif}">{x2;v:training['trname']}</a>					{x2;endtree}				</div>				<h3 style="margin-top: 0px;" class="alert alert-danger">您的余额：{x2;$_user['usercoin']}元<a class="btn btn-primary pull-right" href="index.php?finance-agent-basics-recharge">充值</a> </h3>				{x2;if:count($prices)}				<div class="panel panel-default">					<div class="panel-heading">套餐</div>					<div class="panel-body">						<form action="index.php?finance-agent-basics-openpackage" method="post">							<table class="table table-bordered table-hovered">								<tr class="info">									<th width="30">-</th>									<th>套餐名</th>									<th width="100">代理价</th>									<th width="100">原价</th>									<th width="100">时长</th>								</tr>                                {x2;tree:$prices,price,pid}								<tr>									<td><label for="tr_{x2;v:key}_price"><input id="tr_{x2;v:key}_price" type="radio" name="opentype" value="{x2;v:key}"{x2;if:!v:key} checked{x2;endif}></label></td>									<td><label for="tr_{x2;v:key}_price">{x2;v:price['name']}</label></td>									<td><label for="tr_{x2;v:key}_price">{x2;v:price['agentprice']}元</label></td>									<td><label for="tr_{x2;v:key}_price"><del>{x2;v:price['price']}元</del></label></td>									<td><label for="tr_{x2;v:key}_price">{x2;v:price['time']}天</label></td>								</tr>								{x2;endtree}							</table>							<div class="form-group">								<div class="controls">									<span class="form-inline">手机号：<input type="text" needle="needle" class="form-control" name="phonenumber" msg="请填写手机号" placeholder="手机号"/></span>									<span class="form-inline">&nbsp;&nbsp;姓名：<input type="text" needle="needle" msg="请填写姓名" class="form-control" name="name" placeholder="姓名"/></span>									<span class="form-inline">&nbsp;&nbsp;邮箱：<input type="text" needle="needle" msg="请填写邮箱" class="form-control" name="email" placeholder="邮箱"/> @qq.com</span>									<input type="hidden" name="trid" value="{x2;$training['trid']}"/>&nbsp;&nbsp;									<button class="btn btn-primary" type="submit">购买</button>								</div>							</div>						</form>					</div>				</div>				{x2;endif}				{x2;tree:$subjects,subject,sid}				{x2;tree:$basics[v:subject['subjectid']],basic,bid}				<div class="panel panel-default">					<div class="panel-heading">{x2;v:basic['basic']}</div>					<div class="panel-body">						<form action="index.php?finance-agent-basics-openbasic" method="post">							<table class="table table-bordered table-hovered">								<tr class="info">									<th width="30">-</th>									<th>产品</th>									<th width="100">代理价</th>									<th width="100">原价</th>									<th width="100">时长</th>								</tr>                                {x2;tree:$bprices[v:basic['basicid']],price,pid}								<tr>									<td><label for="basic_{x2;v:basic['basicid']}_{x2;v:key}_price"><input id="basic_{x2;v:basic['basicid']}_{x2;v:key}_price" type="radio" name="opentype" value="{x2;v:key}"{x2;if:!v:key} checked{x2;endif}></label></td>									<td><label for="basic_{x2;v:basic['basicid']}_{x2;v:key}_price">{x2;v:price['name']}</label></td>									<td><label for="basic_{x2;v:basic['basicid']}_{x2;v:key}_price">{x2;v:price['agentprice']}元</label></td>									<td><label for="basic_{x2;v:basic['basicid']}_{x2;v:key}_price"><del>{x2;v:price['price']}元</del></label></td>									<td><label for="basic_{x2;v:basic['basicid']}_{x2;v:key}_price">{x2;v:price['time']}天</label></td>								</tr>                                {x2;endtree}							</table>							<div class="form-group">								<div class="controls">									<span class="form-inline">手机号：<input type="text" needle="needle" class="form-control" name="phonenumber" msg="请填写手机号" placeholder="手机号"/></span>									<span class="form-inline">&nbsp;&nbsp;姓名：<input type="text" needle="needle" msg="请填写姓名" class="form-control" name="name" placeholder="姓名"/></span>									<span class="form-inline">&nbsp;&nbsp;邮箱：<input type="text" needle="needle" msg="请填写邮箱" class="form-control" name="email" placeholder="邮箱"/> @qq.com</span>									<input type="hidden" name="subjectid" value="{x2;v:subject['subjectid']}"/>									<input type="hidden" name="basicid" value="{x2;v:basic['basicid']}"/>&nbsp;&nbsp;									<button class="btn btn-primary" type="submit">购买</button>								</div>							</div>						</form>					</div>				</div>                {x2;endtree}				{x2;endtree}			</div>		</div>	</div></div>{x2;include:footer}</body></html>